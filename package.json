{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "fix-lint": "eslint . --ext ts --ext tsx --ext js --fix",
    "format": "prettier --write .",
    "test-all": "yarn run check-format && yarn run check-lint && yarn run check-types && yarn run build",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@react-google-maps/api": "^2.18.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@types/react-select": "^5.0.1",
    "axios": "^1.3.6",
    "chart.js": "^4.3.0",
    "formik": "^2.2.9",
    "google-map-react": "^2.2.0",
    "haversine-distance": "^1.2.1",
    "i": "^0.3.7",
    "jwt-decode": "^3.1.2",
    "npm": "^9.6.5",
    "or": "^0.2.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-google-maps": "^9.4.5",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.43.7",
    "react-icons": "^4.8.0",
    "react-loader-spinner": "^5.3.4",
    "react-redux": "^8.0.5",
    "react-router": "^6.9.0",
    "react-router-dom": "^6.9.0",
    "react-select": "^5.7.3",
    "react-toastify": "^9.1.2",
    "recompose": "^0.30.0",
    "socket.io-client": "^4.6.1",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@types/google-map-react": "^2.1.7",
    "@types/node": "^18.15.10",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/recompose": "^0.30.10",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tailwindcss": "^3.10.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.5",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-alias": "^0.0.6",
    "vitest": "^0.29.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run pre-commit"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    ".": [
      "yarn run check-format",
      "yarn run check-lint",
      "yarn run check-types"
    ]
  },
  "repository": "https://github.com/vite-react-ts-eslint-prettier-tailwind",
  "author": "Team 5"
}
